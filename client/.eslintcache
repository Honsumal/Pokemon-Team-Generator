   console.log(input)\n    try {\n      const {data} = await newTeam({\n        variables: {nickname:input}\n      });\n    } catch (err) {\n      console.error(err);\n      console.log(error, 'Wong Sum Ting')\n    }\n    window.location.reload()\n  }\n\n  const handleInput = (event) => {\n    event.preventDefault()\n    setInput(event.target.value)\n  }\n\n\n  const style = {\n    position: 'absolute',\n    top: '50%',\n    left: '50%',\n    transform: 'translate(-50%, -50%)',\n    width: 400,\n    bgcolor: 'background.paper',\n    border: '2px solid #000',\n    boxShadow: 24,\n    p: 4,\n  };\n\n  return (\n    <main>\n      <div className=\"flex-row justify-center\">\n        <div className=\"col-12 col-md-10 my-3\">\n          {loading ? (\n            <div>Loading...</div>\n          ) : (\n            <TeamsHome\n              teams={teams}\n              title=\"Here are your currently saved teams...\"\n            />\n          )}\n        </div>\n        <Modal\n          open = {open}\n          onClose = {handleClose}\n          aria-labelledby=\"newteammaker\"\n          aria-describedby=\"new team button\"\n        >\n          <Box sx={style}>\n              <Typography variant=\"h6\" component=\"h3\">\n                Enter New Team Nickname:\n              </Typography>\n              <form onSubmit={handleSubmit}>\n                <TextField sx={{input:{color: '#003049', width: 300}}} onInput={handleInput} />\n                <Button type='submit' sx={{marginTop:1}} variant='contained'>Enter New Team Nickname</Button>\n              </form>\n          </Box>\n        </Modal>\n      </div>\n      <div className='flex-row justify-center my-3'>\n          <Box>\n            <Button variant=\"contained\" onClick={handleOpen}>New Team</Button>\n          </Box>\n      </div>\n\n    </main>\n  );\n};\n\nexport default Home;\n","D:\\Other Projects\\Pokemon-Team-Generator\\client\\src\\components\\Footer\\index.js",[],"D:\\Other Projects\\Pokemon-Team-Gene